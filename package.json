{
  "name": "5nag",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jquery": "^3.2.1",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "npm-run-all -p watch-sass start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-sass && react-scripts build",
    "build-sass": "node-sass sass/main.sass public/styles/main.css --output-style compressed",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-sass": "node-sass sass/main.sass public/styles/main.css --watch"
  },
  "devDependencies": {
    "node-sass": "^4.5.3"
  }
}
